<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
>
    <g:HTMLPanel styleName="main">
        <b.html:Div addStyleNames="row">
            <b.html:Div addStyleNames="col-md-2"/>
            <g:Label ui:field="left" styleName="glyphicon glyphicon-arrow-left arrow-left col-md-2"/>
            <b.html:Div ui:field="main" addStyleNames="col-md-4 account-container">

            </b.html:Div>
            <g:Label ui:field="right" styleName="glyphicon glyphicon-arrow-right arrow-right col-md-2"/>
            <b.html:Div addStyleNames="col-md-2"/>
        </b.html:Div>
        <b.html:Div addStyleNames="row">
            <g:Label ui:field="edit" styleName="account-edit-button"/>
        </b.html:Div>
    </g:HTMLPanel>
</ui:UiBinder>