<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:widget="urn:import:com.vypersw.finances.client.widget"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
    <ui:with field="constants" type="com.vypersw.finances.client.i18n.FinancesConstants"/>
    <g:HTMLPanel ui:field="toolbarContainer">
    	<div class="toolbar-main">
    		<div class="toolbar-buttons">
	    		<div class="toolbar-save">
	    			<widget:ToolbarButton ui:field="save" iconStyle="glyphicon glyphicon-floppy-disk"></widget:ToolbarButton>
	    			<widget:ToolbarButton ui:field="refresh" iconStyle="glyphicon glyphicon-refresh"></widget:ToolbarButton>
	    		</div>
	    	</div>
    	</div>
    </g:HTMLPanel>
</ui:UiBinder>