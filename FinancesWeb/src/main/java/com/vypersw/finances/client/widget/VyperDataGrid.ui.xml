<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:widget="urn:import:com.vypersw.finances.client.widget"
             xmlns:b.gwt="urn:import:org.gwtbootstrap3.client.ui.gwt">
    <ui:with field="constants" type="com.vypersw.finances.client.i18n.FinancesConstants"/>
    <g:HTMLPanel ui:field="main">
        <b.html:Div>
            <b.gwt:DataGrid bordered="true" condensed="true" striped="true" hover="true" height="300px" width="100%" ui:field="dataGrid"/>
            <b:Pagination ui:field="pagination"/>
        </b.html:Div>
    </g:HTMLPanel>
</ui:UiBinder>