<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'
			 xmlns:widget="urn:import:com.vypersw.finances.client.widget"
>
              <ui:with field="constants" type="com.vypersw.finances.client.i18n.FinancesConstants"/>
    <g:HTMLPanel ui:field="main" addStyleNames="max-height">
	    <g:HTMLPanel ui:field="navHeader">
	    	<div class="nav-header-app">
	    		<div class="nav-header-content">
					<widget:MenuButton ui:field="add" iconStyle="glyphicon glyphicon-plus options-item"
									   title="{constants.addExpense}"/>
					<widget:MenuButton ui:field="accountManagement" iconStyle="glyphicon glyphicon-folder-open"
									   title="{constants.accountManagement}"/>
					<widget:MenuButton ui:field="transfer" iconStyle="glyphicon glyphicon-retweet"
									   title="{constants.transfer}"/>
					<widget:MenuButton ui:field="reports" iconStyle="glyphicon glyphicon-signal"
									   title="{constants.reports}"/>
					<widget:MenuButton ui:field="userManagement" iconStyle="glyphicon glyphicon-user"
									   title="{constants.userManagement}"/>
					<widget:MenuButton ui:field="logout" iconStyle="glyphicon glyphicon-off"
									   title="{constants.logout}"/>
		    	</div>
	    	</div>
	    </g:HTMLPanel>
        <g:HTMLPanel ui:field="contentPanel" addStyleNames="max-height content-panel">
    	</g:HTMLPanel>
    </g:HTMLPanel>
</ui:UiBinder>